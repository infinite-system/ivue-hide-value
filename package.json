{
  "name": "ivue-hide-value",
  "displayName": "ivue-hide-value",
  "description": "Visually fold `.value` to a middot and smartly delete it with Backspace.",
  "version": "0.1.3",
  "publisher": "your-name-or-scope",
  "engines": { "vscode": "^1.90.0" },
  "categories": ["Other"],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:vue",
    "onCommand:ivueHideValue.backspace"
  ],
  "contributes": {
    
  },
  "scripts": {
    "build": "esbuild src/extension.ts --bundle --platform=node --external:vscode --outfile=dist/extension.js",
    "watch": "esbuild src/extension.ts --bundle --platform=node --external:vscode --outfile=dist/extension.js --watch",
    "package": "vsce package"
  },
  "devDependencies": {
    "esbuild": "^0.23.0",
    "vsce": "latest",
    "typescript": "^5.6.3",
    "@types/vscode": "^1.90.0"
  }
}